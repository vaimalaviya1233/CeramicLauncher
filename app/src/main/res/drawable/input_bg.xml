<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape>
        <gradient android:startColor="#16000000" android:endColor="#60000000" android:angle="90"/>
        <stroke android:color="#07ffffff" android:width="1dp"/>
        <corners android:radius="8dp"/>
    </shape>
</item>
<item
    android:bottom="1dp"
    android:left="1dp"
    android:right="1dp"
    android:top="1dp">
    <shape>
        <stroke android:color="#55000000" android:width="1dp"/>
        <corners android:radius="7.5dp"/>
    </shape>
</item>
</layer-list>